{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        // Application level        
        { "command": "toggleFullscreen", "keys": "ctrl+f" },
        { "command": "toggleFocusMode", "keys": "alt+space" },
        { "command": "toggleAlwaysOnTop", "keys": "alt+w" },
        { "command": "find", "keys": "f2" },

        // Pane Management
        { "command": "closePane", "keys": "alt+shift+q" },
        { "command": { "action": "splitPane", "split": "horizontal" }, "keys": "alt+u" },
        { "command": { "action": "splitPane", "split": "vertical" }, "keys": "alt+o" },
        { "command": { "action": "resizePane", "direction": "down" }, "keys": "alt+ctrl+shift+k" },
        { "command": { "action": "resizePane", "direction": "left" }, "keys": "alt+ctrl+shift+j" },
        { "command": { "action": "resizePane", "direction": "right" }, "keys": "alt+ctrl+shift+l" },
        { "command": { "action": "resizePane", "direction": "up" }, "keys": "alt+ctrl+i" },
        { "command": { "action": "moveFocus", "direction": "down" }, "keys": "alt+k" },
        { "command": { "action": "moveFocus", "direction": "left" }, "keys": "alt+j" },
        { "command": { "action": "moveFocus", "direction": "right" }, "keys": "alt+l" },
        { "command": { "action": "moveFocus", "direction": "up" }, "keys": "alt+i" },
        { "command": { "action": "moveFocus", "direction": "previous" }},
        { "command": { "action": "moveFocus", "direction": "previousInOrder" }, "keys": "alt+-" },
        { "command": { "action": "moveFocus", "direction": "nextInOrder" }, "keys": "alt+plus" },
        { "command": { "action": "moveFocus", "direction": "first" } },
        { "command": { "action": "swapPane", "direction": "down" }, "keys": "alt+shift+k" },
        { "command": { "action": "swapPane", "direction": "left" }, "keys": "alt+shift+j" },
        { "command": { "action": "swapPane", "direction": "right" }, "keys": "alt+shift+l" },
        { "command": { "action": "swapPane", "direction": "up" }, "keys": "alt+shift+i" },
        { "command": { "action": "swapPane", "direction": "previous"} },
        { "command": { "action": "swapPane", "direction": "previousInOrder"} },
        { "command": { "action": "swapPane", "direction": "nextInOrder"} },
        { "command": { "action": "swapPane", "direction": "first" } },
        { "command": "togglePaneZoom", "keys": "alt+f" },
        { "command": "toggleSplitOrientation" },
        { "command": "toggleReadOnlyMode" },

        // Tab Management
        { "command": "closeTab", "keys": "ctrl+shift+q"},
        { "command": "closeOtherTabs" },
        { "command": "closeTabsAfter" },
        { "command": { "action" : "moveTab", "direction": "forward" }, "keys": "ctrl+shift+j"},
        { "command": { "action" : "moveTab", "direction": "backward" }, "keys": "ctrl+shift+l"},
        { "command": "newTab", "keys": "alt+enter" },
        { "command": "newWindow", "keys": "ctrl+shift+n" },        
        { "command": "openTabRenamer", "keys": "shift+f2" },
        { "command": { "action": "newTab", "index": 0 }, "keys": "ctrl+shift+1" },
        { "command": { "action": "newTab", "index": 1 }, "keys": "ctrl+shift+2" },
        { "command": { "action": "newTab", "index": 2 }, "keys": "ctrl+shift+3" },
        { "command": { "action": "newTab", "index": 3 }, "keys": "ctrl+shift+4" },
        { "command": { "action": "newTab", "index": 4 }, "keys": "ctrl+shift+5" },
        { "command": { "action": "newTab", "index": 5 }, "keys": "ctrl+shift+6" },
        { "command": { "action": "newTab", "index": 6 }, "keys": "ctrl+shift+7" },
        { "command": { "action": "newTab", "index": 7 }, "keys": "ctrl+shift+8" },
        { "command": { "action": "newTab", "index": 8 }, "keys": "ctrl+shift+9" },
        { "command": "duplicateTab", "keys": "ctrl+enter" },
        { "command": "nextTab", "keys": "ctrl+l" },
        { "command": "prevTab", "keys": "ctrl+j" },
        { "command": { "action": "switchToTab", "index": 0 }, "keys": "alt+1" },
        { "command": { "action": "switchToTab", "index": 1 }, "keys": "alt+2" },
        { "command": { "action": "switchToTab", "index": 2 }, "keys": "alt+3" },
        { "command": { "action": "switchToTab", "index": 3 }, "keys": "alt+4" },
        { "command": { "action": "switchToTab", "index": 4 }, "keys": "alt+5" },
        { "command": { "action": "switchToTab", "index": 5 }, "keys": "alt+6" },
        { "command": { "action": "switchToTab", "index": 6 }, "keys": "alt+7" },
        { "command": { "action": "switchToTab", "index": 7 }, "keys": "alt+8" },
        { "command": { "action": "switchToTab", "index": 4294967295 }, "keys": "alt+9" },

        // Scrollback
        { "command": "scrollDown", "keys": "ctrl+alt+k" },
        { "command": "scrollDownPage", "keys": "ctrl+alt+l" },
        { "command": "scrollUp", "keys": "ctrl+alt+i" },
        { "command": "scrollUpPage", "keys": "ctrl+alt+j" },
        { "command": "scrollToTop", "keys": "ctrl+alt+u" },
        { "command": "scrollToBottom", "keys": "ctrl+alt+o" },
        { "command": "quakeMode", "keys":"alt+`" }
    ],
